-
  const sec5Data = [
    {
      title: 'Java, Play, Spring, JE8',
      icon: 'java.png',
      middle: '1900',
      senior: '2200',
    },
    {
      title: 'Ruby on rails',
      icon: 'rails.png',
      middle: '1800',
      senior: '2100',
    },
    {
      title: 'JavaScript, React:Redux, Apollo, GraphQL, Node.js',
      icon: 'js.png',
      middle: '1700',
      senior: '2000',
    },
    {
      title: 'Camunda',
      icon: 'camunda.png',
      middle: '1800',
      senior: '2100',
    },
    {
      title: 'Python, Django, Flask, Falcon',
      icon: 'python.png',
      middle: '1600',
      senior: '1800',
    },
    {
      title: 'Mobile apps, Native, Android, IOS',
      icon: 'apps.png',
      middle: '1500',
      senior: '1800',
    },
    {
      title: 'PHP',
      icon: 'php.png',
      middle: '1500',
      senior: '1700',
    }
  ]



+section(id=menu[1].href).sec5.bg-white
  +container
    h3.sec5__title
      != translate.sec5.title

    +slider('sec5')
      each item in sec5Data

        mixin sec5Button(buttonClass = '')
          +button({ fill: true, icon: {name: 'icon_buy_people', class: `sec5__slide-button-icon`} })&attributes(attributes)
            != translate.sec5.buyManButton

        +sliderSlide('sec5')
          .sec5__slider-slide-inner
            .sec5__slider-image-wrap
              img(src=`./img/${item.icon}`, alt="").sec5__slider-image
            .sec5__slide-info
              p.sec5__slide-title
                strong
                  != item.title
              .sec5__slide-item
                p.sec5__slide-experience.
                  Middle
                p.mb-0
                  strong.text-m
                    != item.middle
                  |
                  |
                  strong.
                    ₽ / #{translate.sec5.hour}
              .sec5__slide-item
                p.sec5__slide-experience.
                  Senior
                p.mb-0
                  strong.text-m
                    != item.senior
                  |
                  |
                  strong.
                    ₽ / #{translate.sec5.hour}
              +sec5Button().sec5__slide-button
            .sec5__slider-description
              p.
          .sec5__slide-button-tablet
            +sec5Button()